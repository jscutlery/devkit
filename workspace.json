{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "cypress-harness",
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "cypress-angular": "packages/cypress-angular",
    "cypress-angular-dev-server": "packages/cypress-angular-dev-server",
    "cypress-harness": "packages/cypress-harness",
    "cypress-harness-e2e": "packages/cypress-harness-e2e",
    "cypress-harness-integration": "packages/cypress-harness-integration",
    "cypress-mount": "packages/cypress-mount",
    "cypress-mount-integration": "packages/cypress-mount-integration",
    "sandbox": "packages/sandbox"
  }
}
