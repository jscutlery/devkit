{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "swc-plugin-angular-wide",
  "sourceRoot": "tests/swc-plugin-angular-wide/src",
  "prefix": "lib",
  "tags": [],
  "projectType": "library",
  "targets": {
    "debug": {
      "command": "nx watch --all -- 'rm -Rf node_modules/.vite node_modules/.vitest && yarn jest --clearCache && nx test swc-plugin-angular-wide --skip-nx-cache'"
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "cwd": "{projectRoot}",
        "commands": [
          "jest",
          "echo TODO: \uD83D\uDEA7 vitest --watch false"
        ]
      },
      "dependsOn": [
        {
          "projects": [
            "swc-plugin-angular"
          ],
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
